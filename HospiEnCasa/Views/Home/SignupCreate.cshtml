@{
    ViewData["Title"] = "Signup Create Page";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div class="row">
    <div class="col-md-4"> </div>

    <div class="col-md-4" align="center">
        @if (!string.IsNullOrEmpty(ViewBag.Message))
        {
            <div class="alert alert-@ViewBag.Type" align="center">
                <strong>@ViewBag.Message</strong>
            </div>
        }
        
        <button asp-action="Login" type="submit" class="btn btn-secondary btn-lg btn-block">Volver</button>

    </div>

    <div class="col-md-4"> </div>
</div>