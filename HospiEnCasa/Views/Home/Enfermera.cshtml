        </div>

        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target=".navbar-collapse" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>

        <div class="navbar-collapse collapse col-12 col-lg-4">
            <ul class="navbar-nav">
                <li class="nav-item ml-lg-5 mr-lg-5">
                    <a class="nav-link text-dark active" asp-area="" asp-controller="Home" asp-action="Home">Home</a>
                </li>
                <li class="nav-item mr-lg-5">
                    <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Paciente">Paciente</a>
                </li>
                <li class="nav-item mr-lg-5">
                    <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Medico">Medico</a>
                </li>
                <li class="nav-item ">
                    <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Enfermera">Enfermera</a>
                </li>
            </ul>
        </div>
        <div class="navbar-collapse collapse col-12 col-lg-4">
            <ul class="navbar-nav ml-auto nav-flex-icons">
                <li class="nav-item avatar dropdown">
                    <a class="nav-link dropdown-toggle" id="navbarDropdownMenuLink-55" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                        <img src="https://mdbootstrap.com/img/Photos/Avatars/avatar-2.jpg" class="rounded-circle z-depth-0" alt="avatar image" width="35" height="35">
                    </a>
                    <div class="dropdown-menu dropdown-menu-lg-right dropdown-secondary" aria-labelledby="navbarDropdownMenuLink-55">
                        <a class="dropdown-item" href="#">Perfil</a>
                        <a class="dropdown-item" href="#">Configuracion</a>
                        <a class="dropdown-item" asp-area="" asp-controller="Home" asp-action="Login">Log out</a>
                    </div>
                </li>
            </ul>
        </div>
    </nav>
</header>


<main>
    <div class="table-responsive-lg p-5 pt-3">
        <div class="align-items-start pb-2">
            <h3 class="pb-1">Acontinuacion se muestra en la tabla todas las enfermestas registradas en el sistema</h3>
            <a class="btn btn-secondary" asp-controller="Home" asp-action="Enfermera_crear">Create Enfermera</a>
        </div>
        <table class="table table-sm">
            <thead>
                <tr>
                    <th scope="col">
                        @Html.DisplayNameFor(model => model.Id)
                    </th>
                    <th scope="col">
                        @Html.DisplayNameFor(model => model.Nombre)
                    </th>
                    <th scope="col">
                        @Html.DisplayNameFor(model => model.Apellido)
                    </th>
                    <th scope="col">
                        @Html.DisplayNameFor(model => model.Especialidad)
                    </th>
                    <th scope="col">
                        @Html.DisplayNameFor(model => model.Cedula)
                    </th>
                    <th scope="col">
                        @Html.DisplayNameFor(model => model.NumeroTelefono)
                    </th>
                    <th scope="col">
                        @Html.DisplayNameFor(model => model.Edad)
                    </th>
                    <th scope="col">
                        @Html.DisplayNameFor(model => model.Genero)
                    </th>

                    @*<th scope="col">
                        @Html.DisplayNameFor(model => model.Password)
                    </th>
                    <th scope="col">
                        @Html.DisplayNameFor(model => model.Mail)
                    </th>*@
                    <th></th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td scope="row">
                            @Html.DisplayFor(modelItem => item.Id)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Nombre)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Apellido)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Especialidad)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Cedula)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.NumeroTelefono)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Edad)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Genero)
                        </td>
                        @*<td>
                            @Html.DisplayFor(modelItem => item.Password)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Mail)
                        </td>*@
                        <td>
                            <div class="btn-group">
                                <button type="button" class="btn btn-light btn-outline-secondary dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                    Opciones
                                </button>
                                <div class="dropdown-menu dropdown-menu-right">
                                    <a class="dropdown-item" asp-controller="Home" asp-action="Enfermera_crear">Edit</a>
                                    <a class="dropdown-item" asp-controller="Home" asp-action="Enfermera_crear">Delete</a>
                                    <a class="dropdown-item" asp-controller="Home" asp-action="Enfermera_crear">Details</a>
                                </div>
                            </div>
                            @*@Html.ActionLink("Edit", "Edit", new { /* id=item.PrimaryKey */ }) |
                            @Html.ActionLink("Details", "Details", new { /* id=item.PrimaryKey */ }) |
                            @Html.ActionLink("Delete", "Delete", new { /* id=item.PrimaryKey */ })*@
                        </td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</main>


